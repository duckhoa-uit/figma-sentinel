{
  "name": "@khoavhd/figma-sentinel-core",
  "version": "1.1.0",
  "description": "Core library for Figma Sentinel - Design change tracking",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm && tsc --emitDeclarationOnly --declaration --outDir dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "figma",
    "design-ops",
    "core"
  ],
  "author": "duckhoa-uit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/duckhoa-uit/figma-sentinel.git",
    "directory": "packages/core"
  },
  "dependencies": {
    "fast-glob": "^3.3.3",
    "p-limit": "^6.2.0",
    "zod": "^3.24.4"
  }
}
