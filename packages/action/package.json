{
  "name": "@khoavhd/figma-sentinel-action",
  "version": "1.0.1",
  "description": "GitHub Action for Figma Sentinel - Automated design sync and PR creation",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "action.yml"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs && tsc --emitDeclarationOnly --declaration --outDir dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --passWithNoTests"
  },
  "keywords": [
    "figma",
    "github-action",
    "design-ops"
  ],
  "author": "duckhoa-uit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/duckhoa-uit/figma-sentinel.git",
    "directory": "packages/action"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@khoavhd/figma-sentinel-core": "^1.0.0"
  }
}
